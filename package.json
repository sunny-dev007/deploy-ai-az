{
  "name": "simple-reactjs-app",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "engines": {
    "node": ">=20.18.0 <21",
    "npm": ">=9.9.0 <10"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.7.1",
    "@azure/msal-react": "^2.0.8",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "@mui/x-data-grid": "^6.19.2",
    "@mui/x-date-pickers": "^6.19.2",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^10.18.0",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^10.0.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.21.3",
    "react-scripts": "5.0.1",
    "remark-gfm": "^4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:backend": "node backend/server.js",
    "start:dev": "concurrently \"npm run start\" \"npm run start:backend\"",
    "start:prod": "node backend/server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "setup-db": "node backend/scripts/setup-db.js"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(axios|framer-motion|react-markdown|remark-gfm|@azure)/)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "gh-pages": "^5.0.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1"
  }
}
